{
    "mcpServers": {
        "shadcn": {
            "command": "npx",
            "args": [
                "shadcn@latest",
                "mcp"
            ]
        },
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        },
        "supabase": {
            "command": "npx",
            "args": [
                "supabase",
                "mcp"
            ]
        }
    }
}