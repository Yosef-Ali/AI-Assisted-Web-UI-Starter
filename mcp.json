{
    "mcpServers": {
        "nextjs": {
            "command": "npx",
            "args": [
                "@vercel-labs/mcp-server-nextjs"
            ]
        },
        "shadcn": {
            "command": "npx",
            "args": [
                "shadcn@latest",
                "mcp"
            ]
        },
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        }
    }
}